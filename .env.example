# Configuration du serveur Minecraft Prominence II
# ================================================

# Adresse IP du serveur (pour les connexions réseau)
# Remplacez par votre adresse IP
SERVER_IP=YOUR_SERVER_IP

# Port du serveur Minecraft
SERVER_PORT=YOUR_SERVER_PORT

# Configuration Java
JAVA_ARGS=-Xmx4G -Xms4G

# Version Minecraft
MINECRAFT_VERSION=1.20.1

# Configuration Fabric
MODLOADER=Fabric
MODLOADER_VERSION=0.16.14

# Configuration JDK
JDK_VENDOR=temurin
RECOMMENDED_JAVA_VERSION=17

# Configuration serveur
RESTART=true
EULA=true
WAIT_FOR_USER_INPUT=false
SKIP_JAVA_CHECK=false

# Configuration Fabric
FABRIC_INSTALLER_VERSION=1.1.0
USE_SSJ=true
SERVERSTARTERJAR_FORCE_FETCH=true
SERVERSTARTERJAR_VERSION=latest

# Configuration JABBA
JABBA_INSTALL_VERSION=0.13.0

# Arguments supplémentaires
ADDITIONAL_ARGS=-Dlog4j2.formatMsgNoLookups=true